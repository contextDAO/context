    "esbuild": "^0.14.38",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "minimist": "^1.2.6",
    "node-typescript": "^0.1.3",
    "prettier": "^2.6.2",
    "replace-in-file": "^6.3.2",
